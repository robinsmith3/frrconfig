frr version 8.4_git
frr defaults traditional
hostname frr-3
no ipv6 forwarding
!
interface eth1
 ip address 10.1.3.1/24
exit
!
interface eth2
 ip address 10.2.3.2/24
exit
!
interface lo
 ip address 1.1.1.3/32
 ip router isis 1
 isis passive
exit
!
router bgp 65003
 bgp router-id 1.1.1.3
 no bgp ebgp-requires-policy
 neighbor 10.1.3.3 remote-as 65001
 neighbor 10.2.3.3 remote-as 65002
 !
 address-family ipv4 unicast
  network 1.1.1.3/32
 exit-address-family
exit
!
